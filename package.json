{
  "scripts": {
    "install-all": "npm install && npm install --prefix api && npm install --prefix client",
    "client": "cd client && npm run dev",
    "server": "cd api && npm start",
    "dev": "concurrently \"npm run client\" \"npm run server\""
  },
  "dependencies": {
    "concurrently": "^9.2.0"
  }
}
